<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Dubs Bad Hygiene</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>Defs/RecipeDef[defName="Make_SalineBag"]/ingredients</xpath>
          <value>
            <li>
              <filter>
                <thingDefs>
                  <li>DBH_WaterBottle</li>
                </thingDefs>
              </filter>
              <count>2</count>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>Defs/RecipeDef[defName="Make_SalineBagFive"]/ingredients</xpath>
          <value>
            <li>
              <filter>
                <thingDefs>
                  <li>DBH_WaterBottle</li>
                </thingDefs>
              </filter>
              <count>10</count>
            </li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>